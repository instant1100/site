{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "$Time/*": ["modules/Time/*"],
      "$Jest/*": ["modules/Jest/*"],
      "$TimeTracer/*": ["modules/TimeTracer/*"],
      "$Typing/*": ["modules/Typing/*"],
      "$Lessons/*": ["modules/Lessons/*"],
      "$Tehnolog/*": ["modules/Tehnolog/*"]
    }
  }
}